<UserControl x:Class="CINEMA_ORM.UserControls.AgregarCartel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CINEMA_ORM.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="GD_CONTENIDO">
        <Image x:Name="IM_CARTEL" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button x:Name="BT_OPENFILE" Click="btnOpenFile_Click" Content="Cargar Cartel" Cursor="Hand" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
